{
  "manifest_version": 3,
  "name": "Nemu",
  "description": "",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "commands": {
    "generate-script": {
      "suggested_key": {
        "default": "Ctrl+Shift+H"
      },
      "description": "Gerar script para a p√°gina"
    }
  },
  "permissions": ["activeTab", "scripting", "storage", "commands"],
  "web_accessible_resources": [
    {
      "resources": ["/images/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
